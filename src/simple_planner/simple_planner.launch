<launch>
	<node pkg="map_server" type = "map_server" name = "map_server" args = "$(find simple_planner)/my_maps/map.yaml"/>
	<node pkg="simple_planner" type = "planner_node" name= "planner_node" output = "screen">
	<param name="inflation_radius" value="3"/>
	<param name="cost_scaling_factor" value="10"/>
	</node>
	<node pkg="tf" type="static_transform_publisher" name="map_to_base_link" args="0 0 0 0 0 0 0 map base_link 100"/>
	<node pkg="rviz" type="rviz" name="rviz" args="-d  $(find simple_planner)/planner.rviz"/>
</launch>

